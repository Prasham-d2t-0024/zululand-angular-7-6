<header class="header bg-white">
  <nav role="navigation" [attr.aria-label]="'nav.user.description' | translate" class="container-fluid navbar navbar-expand-md p-0">
    <div class="d-flex flex-grow-1">
      <a class="navbar-brand m-2" routerLink="/home">
        <img class="w-100" [src]="logoPath" [attr.alt]="'menu.header.image.logo' | translate" [ngClass]="!!(isXsOrSm$ | async) ? 'mobilelogoheight' : ''" />
      </a>
    </div>
    <div class="d-flex flex-grow-1 ml-auto justify-content-end align-items-center mb-1 xs-action-menu">
      <ds-themed-search-navbar></ds-themed-search-navbar>
      <!-- <ds-lang-switch class="mx-2"></ds-lang-switch> -->
      <!-- <ds-context-help-toggle class="mx-2"></ds-context-help-toggle> -->
      <ds-themed-auth-nav-menu></ds-themed-auth-nav-menu>
      <ds-impersonate-navbar></ds-impersonate-navbar>
      <div class="">
        <button class="navbar-toggler" type="button" (click)="toggleNavbar()"
                aria-controls="collapsingNav"
                aria-expanded="false" [attr.aria-label]="'nav.toggle' | translate">
          <span class="navbar-toggler-icon fas fa-bars fa-fw" aria-hidden="true"></span>
        </button>
      </div>
    </div>
  </nav>
  <ds-themed-navbar></ds-themed-navbar>

</header>
