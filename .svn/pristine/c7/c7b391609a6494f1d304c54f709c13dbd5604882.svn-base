
.bottem-count-text ul {
    display: flex;
    flex-direction: row;
    align-items: center;
    padding: 0;
    margin: 0;
}

.bottem-count-text ul li {
    text-decoration: none;
    list-style: none;
    display: flex;
    align-items: center;
    width: 100%;
    justify-content: center;
    padding: 10px;
}

.bottem-count-text ul li img {
    width: 28px !important;
    margin-right: 4px;
}

.bottem-count-text ul li span {
    font-size: 14px;
    font-weight: 600;
    color: #978f8f;
    margin-left: 5%;
}

.font-wight-info {
    font-weight: 600;
}