<div class="col-12" style="max-height: 100vh;overflow-y: auto;overflow-x: auto;" *ngIf="itemTab">
    <div class="row mb-2 p-0 item-list-abstract" *ngIf="itemTab.firstMetadataValue('dc.title')">
        <span class="col-lg-12 text-secondary font-weight-bold">Title  </span>
        <ng-container *ngFor="let title of itemTab.allMetadataValues('dc.title'); let last = last;">
          <span class="col-lg-12 slash text-black">{{title}}</span>
          <span *ngIf="!last"> | </span>
        </ng-container>
      </div>
    <div class="col-lg-12 mb-2 p-0" *ngIf="itemTab.firstMetadataValue('dc.contributor.author')">
        <span class="text-secondary  h6">Authors : </span>
        <ng-container *ngFor="let author of itemTab.allMetadataValues('dc.contributor.author'); let last = last;">
            <a [routerLink]="['/search']" [queryParams]="{'f.author':author+',equals'}">{{author}}</a>
            <span *ngIf="!last"> | </span>
        </ng-container>
    </div>
    <div class="col-lg-12 mb-3 p-0" *ngIf="itemTab.firstMetadataValue('dc.contributor.editor')">
        <span class="text-secondary  h6">Editor(s) : </span>
        <ng-container *ngFor="let language of itemTab.allMetadataValues('dc.contributor.editor'); let last = last;">
            <span class="text-black">{{language}}</span>
            <span *ngIf="!last"> | </span>
        </ng-container>
    </div>
    <div class="col-lg-12 mb-3 p-0" *ngIf="itemTab.firstMetadataValue('dc.contributor.corpauthor')">
        <span class="text-secondary  h6">Corporate Author : </span>
        <ng-container *ngFor="let language of itemTab.allMetadataValues('dc.contributor.corpauthor'); let last = last;">
            <span class="text-black">{{language}}</span>
            <span *ngIf="!last"> | </span>
        </ng-container>
    </div>
    <div class="col-lg-12 mb-3 p-0" *ngIf="itemTab.firstMetadataValue('dc.date.issued')">
        <span class="text-secondary  h6">Year of Publication : </span>
        <span class="text-black">{{itemTab.firstMetadataValue('dc.date.issued')}}</span>
    </div>
    <div class="col-lg-12 mb-2 p-0" *ngIf="itemTab.firstMetadataValue('dc.language.iso')">
        <span class="text-secondary  h6">Language : </span>

        <ng-container *ngFor="let subject of itemTab.allMetadataValues('dc.language.iso'); let last = last;">
            <a class="h6" [routerLink]="['/search']" [queryParams]="{'f.language':subject+',equals'}"><span
                    class="badge badge-secondary  ">{{subject}}</span></a>
            <span *ngIf="!last"> | </span>
        </ng-container>
    </div>
    <div class="col-lg-12 mb-2 p-0" *ngIf="itemTab.firstMetadataValue('dc.subject')">
        <span class="text-secondary  h6">Keywords : </span>
        <ng-container *ngFor="let subject of itemTab.allMetadataValues('dc.subject'); let last = last;">
            <a class="h6" [routerLink]="['/search']" [queryParams]="{'f.subject':subject+',equals'}"><span
