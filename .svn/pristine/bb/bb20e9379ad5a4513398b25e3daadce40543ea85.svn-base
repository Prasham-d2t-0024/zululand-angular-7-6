<div class="container login-container user-manage-container mt-4">
  <div class="row align-items-center justify-content-center text-center">
    <div class="col-12 col-sm-8 col-md-6 col-lg-4 mx-auto">
      <div class="login-card usermange-card">
        <div class="px-4 pt-3 mx-auto">
          <div class="mb-3 w-50 h-50 login-branding">
            <img style="margin-left: 90px;" class="w-100" [src]="logoPath" alt="{{'repository.image.logo' | translate}}">
          </div>
          <h4 class="mb-0 font-weight-normal">{{"login.form.header" | translate}}</h4>
        </div>
        <ds-log-in [isStandalonePage]="true"></ds-log-in>
      </div>
    </div>
  </div>
</div>