<footer class="text-white">
  <div *ngIf="showTopFooter" class="footerTop">
    <!-- Grid container -->
    <div class="container-fluid">
      <!--Grid row-->
      <div class="row align-items-md-baseline">
        <div class="col-12 col-md-6 col-lg-3 order-md-1 order-lg-1 footerCol">
          <!--Grid column-->
          <div class="footer-col-top mb-3">
            <img class="footerLogo mb-4" src="assets/images/panoro-logo.jpg"
                  [attr.alt]="'menu.header.image.logo' | translate" />
            <h5 class="d-flex align-items-center justify-content-center">              
                <span>Repository Centeral Discovery System(Repository)</span>
            </h5>
          </div>
          <div class="footer-col-middle">
            <p>International Institute for primary Health Care-Ethiopia</p>
            <p>
              <span>Addis Ababa, Ethiopia </span> |
              <span> resourcecenter@iphce.org </span>
            </p>
            <p>
              <a target="_blank" href="https://iphce.org/">Copyright © IPHCE</a>
            </p>
          </div>
          <!-- End Grid column-->
        </div>
        <div class="col-12 col-md-6 col-lg-3 order-md-3 order-lg-2 footerCol">
          <div class="footer-col-top mb-3">
            <h5 class="col-title"><span>Quick Links</span></h5>
          </div>
          <div class="footer-col-middle">
            <ul class="list-unstyled mb-0">
              <li class="mb-2">
                <a href="/aboutus" target="_blank" class="">About Repository</a>
              </li>
              <li class="mb-2">
                <a href="https://iphce.org/iphc-e-resource-center/" target="_blank" class="">Resource Center</a>
              </li>
              <li class="mb-2">
                <a href="https://iphce.org/courses-2/" target="_blank" class="">Courses</a>
              </li>
              <li class="mb-2">
                <a href="https://phcommunityofpractice.org/" target="_blank" class="">CoP</a>
              </li>
            </ul>
          </div>
        </div>
        <div class="col-12 col-md-6 col-lg-3 order-md-4 order-lg-3 footerCol">
          <div class="footer-col-top mb-3">
            <h5 class="col-title"><span>Information Links</span></h5>
          </div>
          <div class="footer-col-middle">
            <ul class="list-unstyled mb-0">
              <li class="mb-2">
                <a href="https://gdpr.eu/privacy-policy" target="_blank" class="">Privacy Policy</a>
              </li>
              <li class="mb-2">
                <a href="https://gdpr.eu/cookies/" target="_blank" class="">Cookies Policy</a>
              </li>
              <li class="mb-2">
                <a href="https://en.wikipedia.org/wiki/Open_educational_resources_policy" target="_blank" class="">Open
                  Educational Resource Policy</a>
              </li>
              <!-- <li class="mb-2">
                <a href="https://en.wikipedia.org/wiki/Open_educational_resources_policy" class=""></a>
              </li> -->
            </ul>
          </div>
        </div>
        <div class="col-12 col-md-6 col-lg-3 align-self-md-end align-self-lg-baseline order-md-2 order-lg-4 footerCol">
          <div class="footer-col-top mb-3">
            <h5 class="col-title"><span>Join our mailing list</span></h5>
          </div>
          <div class="footer-col-middle">
            <p>
              Be informed about the most recent information
            </p>
            <div class="row flex-wrap justify-content-center">
              <input type="mail" class="my-2 md-input w-75" (keyup)="validateEmail()" [(ngModel)]="email"
                placeholder="The subscriber's email address">
              <button type="button" class="btn btn-primary btn-sm rounded-pill w-75" [disabled]="disable"
                (click)="subscirbe()">Subscribe</button>
            </div>            
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- End Grid container -->

