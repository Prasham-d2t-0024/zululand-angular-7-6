        position: relative;
        overflow: hidden;
    }

    .iframe-container iframe {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
    }

    iframe {
        max-width: 100%;
    }
    .backbox {
        color: #fff;
        margin-top: 190px;
        border-radius: 15px;
        z-index: 1;
    }
    .banner-header {
        position: relative;
        &::before {
            background-color: rgba(0, 0, 0, .5);
            bottom: 0;
            content: "";
            left: 0;
            position: absolute;
            right: 0;
            top: 0;
        }
    }
    ::ng-deep .backdrop {
        // background-image: url("../../assets/images/banner.jpg");
        background-color: white;
        background-size: cover;
        background-position: center;
        // height: 400px;
        width: 100vw;
        z-index: 1
    }

    .wrapper {
        display: flex;
        align-items: center;
        flex-direction: column;
    }

    .my_class {
        min-height: 100vh;
    }
    .advanceSearchText {
        a {
            font-size: 13px;
            padding-right: 20px;
        }
    }
}

/* Works on Firefox */
* {
    scrollbar-width: thin;
    scrollbar-color: var(--ds-primary-color);
}

/* Works on Chrome, Edge, and Safari */
*::-webkit-scrollbar {
    width: 10px;
}

*::-webkit-scrollbar-track {
    background: white;
}

*::-webkit-scrollbar-thumb {
    background-color: var(--ds-secondary-color);
    border-radius: 20px;
    border: 2px solid white;
}

.carousel-section {
background: linear-gradient(90deg, var(--ds-primary-color) 0%, var(--ds-primary-color) 44%, var(--ds-secondary-color) 69%, var(--ds-secondary-color) 89%);
    .carouselsection-row {
        overflow: hidden;
        margin: 10px 15px 15px;

        .image-card {
            cursor: pointer;
            color: white;
            &::before, &::after {
                display: none;
                background-color: #fff;
                content: "";
                height: 1px;
                left: 20%;
                position: absolute;
                width: 60%;
            }
            &::after {                
                top: 0;
                transition: all .6s;
            }
            &::before {                
                bottom: 0;
                transition: all .6s;
            }

            .image-placeholder {
                text-align: center;

                .img {
                    align-items: center;
                    background-color: white;
                    border: 1px solid white;
                    border-radius: 50%;
                    color: var(--ds-primary-color);
                    display: flex;
                    font-size: 25px;
                    height: 50px;
                    justify-content: center;
                    padding: 10px;
                    transition: all .4s;
                    width: 50px;
                }
            }

            &:hover {
                &::after, &::before  { 
                    display: block; 
                }
                .image-placeholder {
                    .img {
                        background-color: transparent;
                        color: white;
                    }
                }
            }

            .image-content {
                margin: 10px 0 0 0;
                padding: 0 20px;
                position: relative;
                transition: all .4s;

                .card-title {
                    font-size: 18px;
                }

                .card-subtitle {}
            }
        }

    }
}

:host::ng-deep {

    .carousel-control-prev-icon,
    .carousel-control-next-icon {
        height: 22px;
        width: 40px;
    }

    // .carousel-control-prev-icon {
    //     background-image:url("../../assets/images/left-arrow.png");
    // }
    // .carousel-control-next-icon {
    //     background-image: url("../../assets/images/right-arrow.png")
    // }
    .carousel-control-prev,
    .carousel-control-next {
        width: 5%;

        @media screen and (max-width: map-get($grid-breakpoints, lg)) {
            width: 15%;
        }
    }

    .carousel-indicators li {
        display: none !important;
        border-radius: 100% !important;
        background-color: #181818 !important;
        width: 16px !important;
        height: 16px !important;
        opacity: 1 !important;
        margin-right: 14px !important;
    }
}


