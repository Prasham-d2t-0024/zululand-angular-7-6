<div class="row">
    <div class="col-md-12" style="background-color: white;">
<div class="tab-nav-band menu-with-filter">
    <div class="tab-main-menu-band">
        <button class="tablink current"
            onclick="WorkSPaceParentTab(event,'Action')"><span class="lead">{{"search.filters.head" | translate}}</span></button>
        <button class="tablink"
            onclick="WorkSPaceParentTab(event,'Bookmarks')"><span class="lead">Advanced Search</span></button>
    </div>
</div>
<div id="Action" class="text-band" style="display: block;">
    <div *ngIf="(filters | async)?.hasSucceeded">
        <div *ngFor="let filter of (filters | async)?.payload; trackBy: trackUpdate">
            <ds-search-filter [filter]="filter" [inPlaceSearch]="inPlaceSearch" [refreshFilters]="refreshFilters"></ds-search-filter>
        </div>
    </div>
</div>
<div id="Bookmarks" class="text-band">
    <ds-advanced-search *ngIf="appConfig.search.advancedFilters.enabled"
    [inPlaceSearch]="inPlaceSearch"></ds-advanced-search>
</div>
</div>
</div>
