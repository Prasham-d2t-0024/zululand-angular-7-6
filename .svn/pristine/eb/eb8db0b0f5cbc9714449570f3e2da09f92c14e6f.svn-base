body {
    padding: 0;
    margin: 0 auto;
    font-family: 'Roboto', sans-serif;
    background-color: #FAFAFA;
}

.section {
    margin: 30px 30px;
}
.d-flex{
    display: flex;
}

/*upload file*/
.files input {
    transition: outline-offset .15s ease-in-out, background-color .15s linear;
    padding: 0px;
    text-align: center !important;
    margin: 0;
    width: 100% !important;
    height: 111px!important;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 27px 140px;
    background-color: #f2f2f2;
    border: 1px solid #cecdcdbd!important;
}
.files input:focus{     outline: 2px dashed #92b0b3;  outline-offset: -10px;
    -webkit-transition: outline-offset .15s ease-in-out, background-color .15s linear;
    transition: outline-offset .15s ease-in-out, background-color .15s linear; border:1px solid #92b0b3;
 }
.files{ position:relative}
.files:after {  pointer-events: none;
    position: absolute;
    top: 60px;
    left: 0;
    width: 50px;
    right: 0;
    height: 56px;
    content: "";
    display: block;
    margin: 0 auto;
    background-size: 100%;
    background-repeat: no-repeat;
}
.color input{ background-color:#f1f1f1;}
.files:before {
    position: absolute;
    pointer-events: none;
    width: 100%;
    right: 0;
    content: " Click here to Upload or drop your file here ";
    display: block;
    margin: 0 auto;
    color: #161616;
    font-weight: 400;
    text-align: center;
    font-size: 19px;
    transform: translate(-50%, -50%);
    left: 50%;
    top: 79%;
    font-size: 17px;
}
/*upload file --End*/


/*accordion*/


.pdf-viewer{
    background-color: #000;
}
.accordion-band .accordion-button:not(.collapsed) {
    color: #ffffff;
    background-color: #0070C0;
    box-shadow: inset 0 -1px 0 rgb(0 0 0 / 13%);
}
.accordion-band button.accordion-button {
    background-color: #0070C0;
}
.accordion-item .accordion-button {
    border-radius: 0px;
}
.accordion-item:first-of-type .accordion-button{
    border-radius: 0px;
}

.accordion-item .accordion-button {
    border-radius: 0px;
    height: 35px;
    font-size: 18px;
    padding: 0px 8px;
    font-weight: 500;
    color: #fff;
}

.accordion-button::after {
    margin-left: auto;
    content: "";
    background-image: unset;
    background-repeat: no-repeat;
    background-size: 1.25rem;
    transition: transform 0.2s ease-in-out;
    background: none;
    width: 0;
    height: 0;
    bottom: 24px;
    right: 10px;
    border-left: 11px solid transparent;
    border-right: 12px solid transparent;
    border-top: 10px solid #ffffff;
}
.form-group label {
    font-size: 18px;
    font-weight: 400;
    color: #161616;
    margin-bottom: 1px;
}

.form-group input {
    border-radius: 0px;
    height: 44px;
    border: 1px solid #eee;
    font-size: 18px;
}
.form-control:disabled, .form-control[readonly] {
    background-color: #F2F2F2;
    opacity: 1;
    color: #919090;
}
.form-group {
    margin-bottom: 12px;
}
.accordion-item {
    background-color: #fff;
    border: none;
}
.form-group select{
    border-radius: 0px;
    height: 44px;
    border: 1px solid #eee;
    font-size: 18px;
}
.btn-blue button.btn-primary {
    background-color: #0070c0;
    padding: 10px;
    border-radius: 10px;
    width: 100%;
    border: none;
}
ul li {
    list-style: none;
}
ul {
    margin: 0;
    padding: 0;
}
.text-card-band {
    display: flex;
    justify-content: space-between;
    border: 1px solid #eee;
    padding: 10px 12px;
    margin-top: 16px;
}
.text-card-band .left-panel h3 {
    color: #161616;
    font-size: 20px;
    font-weight: 600;
    text-decoration: underline;
    margin-bottom: 1px;
}

.text-card-band .left-panel p {
    color: #161616;
    font-size: 20px;
    margin: 0;
    line-height: 24px;
}
.text-card-band .right-panel i {
    font-size: 24px;
    color: #0070c0;
    margin-left: 10px;
}


.btn-blue.w-auto.save-btn button{
    width: 127px;
}

.btn-blue.w-auto.save-btn {
    margin-top: 14px;
}
.btn-group ul{
        display: flex;
}
.btn-group{
